{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.4"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage:main"
      }
    ]
  }